<!DOCTYPE html>
<html>
  <head>
    <title>SmartAgent</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>

		<%= stylesheet_link_tag "http://cdn.bootcss.com/jquery-tagsinput/1.3.3/jquery.tagsinput.css" %>
		<%= javascript_include_tag "http://cdn.bootcss.com/jquery-tagsinput/1.3.3/jquery.tagsinput.js" %>
		<%= javascript_include_tag "http://cdn.bootcss.com/jquery-tagsinput/1.3.3/jquery.tagsinput.min.js" %>


    <%= csrf_meta_tags %>
  </head>
  <body>
<%= render 'share/header' %>
    <%= flash[:notice] %>
    <%= yield %>
<%= render 'share/footer' %>
  </body>
</html>
