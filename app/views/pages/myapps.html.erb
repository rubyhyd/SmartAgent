
<%= render "share/header" %>
<div>
	<h1>My apps</h1>
  <% if signin? %>
		<% @apps.each do |f| %>
      <ul>
				<li>name</li>
       	<li><%= f.name %></li>
				<li>id</li>
				<li><%= f.id %></li>
        <li><%= link_to 'Destory', [current_user, f], method: :delete, data: {confirm: 'are you sure?'} %></li>
       </ul>
    <% end %> 
	<% else %>
      <h2 align="center">未登录!</h2>
	<% end %>
</div>
<%= render "share/footer" %>

