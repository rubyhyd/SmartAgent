<div class="main">
  <div class="container">
    <div>
      <h2>Sensors</h2>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Name</th>
            <th>Capbility</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <%= render @sensors %>
        </tbody>
      </table>
    </div>
    <div>
      <h2>Actuators</h2>
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>Name</th>
            <th>Capbility</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
        <%= render @actuators %>
        </tbody>
      </table>
    </div>
  </div>
</div>




