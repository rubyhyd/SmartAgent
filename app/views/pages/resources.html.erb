<!doctype html>
<html>
<head>
  <meta http-equiv="content-type" content="text/html; charset=utf-8" />
  <title>SmartAgent</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <%= stylesheet_link_tag "bootstrap" %>
  <%= stylesheet_link_tag "smartagent" %>
  <%= stylesheet_link_tag "sticky-footer-navbar" %>
</head>

<body>


<div id="wrap">
  <%= render "share/header" %>
  <div class="container">
    <div class="row">
      <div class="input-group input-group-lg col-md-4 col-md-offset-1" style="padding-bottom:40px">
        <input type="text" class="form-control" placeholder="请输入关键字">
				  <span class="input-group-addon">
				  <a href="./search.jsp"><span class="glyphicon glyphicon-search"></span></a>
				  </span>
      </div>
      <a href="./search" class="btn btn-primary" role="button">查看已经激活的资源</a>
    </div>
    <div class="row">
      <div class="col-md-6">
        <img src="img/u68.png" class="img-responsive" />
      </div>
      <div class="col-md-6">
        <img src="img/u32.jpg" class="img-responsive"/>
      </div>
    </div>

  </div>

</div>
<%= render "share/footer" %>


<script src="js/jquery.js"></script>

<script src="js/bootstrap.min.js"></script>
</body>
</html>